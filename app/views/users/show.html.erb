<h1>Welcome! <%=@user.name%> </h1>
<h2>Age: <%=@user.age%></h2>

<ol>
  <%if @orders.length > 0%>


    <%@orders.each do |order|%>
    <h2>Order #<%= @count %></h2>
      <h3>Drinks: </h3>
       <%order.drinks.each do |drink| %>
        <li><%= drink.name%>
          <%end%></li>
          <h3>Food:</h3>
      <%order.foods.each do |food| %>
        <li><%= food.name%>
      <%end%></li>


      <% @count += 1 %>

    <%end%>
    <%end%>

</ol>
