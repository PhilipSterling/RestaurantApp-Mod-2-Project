<%= form_for @user do |f| %>
  Name:       <%= f.text_field :name %><br />
  Username: <%= f.text_field :username %><br />
  Password: <%= f.password_field :password %> <br>
  Confirm Password: <%= f.password_field :password_confirmation %>
  Age : <%= f.number_field :age %><br />
  <%= f.submit %>
<% end %>
